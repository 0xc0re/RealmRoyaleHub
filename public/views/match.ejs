    <!-- Content
    ================================================== -->
    <%if(renderDetails){%>
    <div class="site-content">
        <div class="container" style="max-width: 1000px">

            <div class="card card--has-table">
                <div class="card__header">
                    <h4>Match Stats</h4>
                    <div class="widget-results__title">Region: <%= matchDetails['region'] %></div>
                    <div class="widget-results__title">Match ID: <%= matchDetails['match_id'] %></div>

                    <div class="widget-results__title">
                        <div class="widget-results__title">

                            Date: <%= matchDetails['match_datetime'].split('T')[0] %>
                        </div>
                        <div class="widget-results__title">

                            <%
                            theDate = new Date(Date.parse(matchDetails['match_datetime'] + 'Z'));
                            %>

                            Time: <%= theDate.toLocaleTimeString() %> UTC
                        </div>
                    </div>

                </div>
                <div class="card__content">
                    <div class="table-responsive">
                        <table class="table table-hover" id="example"
                               style="padding-top: 10px;padding-bottom: 10px;text-align: center;align-items: center">
                            <thead>
                            <tr>
                                <th class="team-result__placement" style="text-align: center;max-width: 90px;font-size: 18px">
                                    Placement
                                </th>
                                <th class="team-result__timealive" style="text-align: center;font-size: 18px" >Seconds Alive</th>
                                <th class="team-result__vs" style="max-width: 20px;word-break: break-all;font-size: 18px">Players</th>
                                <th class="team-result__kills" style="text-align: center;font-size: 18px">Kills</th>
                                <th class="team-result__deaths" style="text-align: center;font-size: 18px">Deaths</th>
                                <th class="team-result__assists" style="text-align: center;font-size: 18px">Damage</th>
                                <th class="team-result__absorbed" style="text-align: center;max-width: 100px;font-size: 18px">Damage Taken</th>
                            </tr>
                            </thead>
                            <tbody>
                            <% for(const team in matchDetails['teams']){ %>
                                <tr>
                                    <td class="team-result__kills team-color--2" style="max-width: 90px;font-size: 16px">
                                            <%= matchDetails['teams'][team]['placement'] %>
                                    </td>
                                    <td class="team-result__date" style="font-size: 16px">
                                        <%= matchDetails['teams'][team]['players'][0]['duration_secs'] %>
                                    </td>

                                    <td class="team-result__outcome" style="max-width: 150px;word-break: break-all;" >
                                        <div class="team-result__teams-wrapper">
                                            <!-- Team #1 -->
                                            <div class="team-meta team-meta--first team-meta__info team-meta__name" style="font-size: 16px">
                                                        <% for(const player in matchDetails['teams'][team]['players']){ %>
                                                                <%= matchDetails['teams'][team]['players'][player]['name'] %>
                                                    <br>
                                                        <% } %>
                                            </div>
                                            <!-- Team #1 / End -->


                                        </div>
                                    </td>
                                    <%
                                        // I hate doing this twice, but
                                        // we need the total points to help table sorting if needed
                                    let totalKills = 0;

                                    for(const player in matchDetails['teams'][team]['players']) {
                                        totalKills+= matchDetails['teams'][team]['players'][player]['kills_player']
                                    }

                                    %>
                                    <td data-order="<%=totalKills%>" class="team-result__kills team-meta__name team-color--2" style="font-size: 16px">
                                         <% for(const player in matchDetails['teams'][team]['players']){ %>
                                                        <%= matchDetails['teams'][team]['players'][player]['kills_player'] %>
                                            <br>

                                        <% } %>
                                    </td>
                                    <%
                                        // I hate doing this twice, but
                                        // we need the total points to help table sorting if needed
                                        let totalDeaths = 0;

                                        for(const player in matchDetails['teams'][team]['players']) {
                                            totalDeaths+= matchDetails['teams'][team]['players'][player]['deaths']
                                        }

                                    %>
                                    <td data-order="<%=totalDeaths%>" class="team-result__deaths team-meta__name team-color--2" style="font-size: 16px">
                                            <% for(const player in matchDetails['teams'][team]['players']){ %>
                                                    <%= matchDetails['teams'][team]['players'][player]['deaths'] %>
                                            <br>

                                        <% } %>
                                    </td>


                                    <%
                                        // I hate doing this twice, but
                                        // we need the total points to help table sorting if needed
                                        let totalDamage = 0;

                                        for(const player in matchDetails['teams'][team]['players']) {
                                            totalDamage+= matchDetails['teams'][team]['players'][player]['damage_player']
                                        }

                                    %>
                                    <td data-order="<%=totalDamage%>" class="team-result__assists team-meta__name team-color--2" style="font-size: 16px">
                                            <% for(const player in matchDetails['teams'][team]['players']){ %>
                                                    <%= matchDetails['teams'][team]['players'][player]['damage_player'] %>
                                            <br>

                                        <% } %>
                                    </td>

                                    <%
                                        // I hate doing this twice, but
                                        // we need the total points to help table sorting if needed
                                        let totalDamageTaken = 0;

                                        for(const player in matchDetails['teams'][team]['players']) {
                                            totalDamageTaken+= matchDetails['teams'][team]['players'][player]['damage_taken']
                                        }

                                    %>
                                    <td data-order="<%=totalDamageTaken%>" class="team-result__absorbed team-meta__name team-color--2" style="max-width: 100px;font-size: 16px">
                                            <% for(const player in matchDetails['teams'][team]['players']){ %>
                                                    <%= matchDetails['teams'][team]['players'][player]['damage_taken'] %>
                                            <br>

                                        <% } %>
                                    </td>
                                    <!--                                <td class="team-result__replay">-->
                                    <!--                                    <a href="#" class="btn btn-xs btn-default-alt btn-single-icon btn-circle" data-toggle="tooltip" data-placement="top" title="Watch Replay"><i class="fas fa-play"></i></a>-->
                                    <!--                                </td>-->
                                </tr>
                            <% } %>

                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
            <!-- Last Matches Stats / End -->


        </div>
    </div>
    <%}%>>

