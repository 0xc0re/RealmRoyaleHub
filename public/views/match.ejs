                                            <!-- Team #1 -->
                                            <div class="team-meta team-meta--first team-meta__info team-meta__name" style="font-size: 16px;flex-direction: column;align-items: flex-start">
                                                        <% for(const player in matchDetails[team]['name']){ %>
                                                    <a href="/stats/<%=matchDetails[team]['id'][player]%>" style="display: inherit">
                                                        <%= matchDetails[team]['name'][player] %>
                                                    </a>
                                                <br>
                                                        <% } %>
                                            </div>
                                            <!-- Team #1 / End -->


                                        </div>
                                    </td>
                                    <%
                                        // I hate doing this twice, but
                                        // we need the total points to help table sorting if needed
                                    let totalKills = 0;

                                    for(const kills in matchDetails[team]['kills_player']) {
                                        totalKills+= matchDetails[team]['kills_player'][kills]
                                    }

                                    %>
                                    <td data-order="<%=totalKills%>" class="team-result__kills team-meta__name team-color--2" style="font-size: 16px">
                                         <% for(const kills in matchDetails[team]['kills_player']){ %>
                                                        <%= matchDetails[team]['kills_player'][kills] %>
                                            <br>

                                        <% } %>
                                    </td>
                                    <%
                                        // I hate doing this twice, but
                                        // we need the total points to help table sorting if needed
                                        let totalDeaths = 0;

