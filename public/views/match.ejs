    <div class="container" style="align-items: center;text-align: center;padding-top: 50px">
        <div class="header-search-form" style="width: 30rem">
            <form action="/match" id="search" class="search-form" method='post'>
                <input type="text" id="search" name="search" class="form-control" value=""
                       placeholder="Enter Match Number " style="width: 30rem;height: 5rem;font-size: 1.25rem">
                <button type="submit" class="header-mobile__search-submit" style="width: 3rem;height: 5rem"><i
                            class="fas fa-search"></i></button>
